<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div>
      <% messages.forEach((message) => {%>
        <div>         
           <%= message.user %> 
        </div>
        <div>
          <%= message.text %> 
        </div>
        <div>
          <%= message.added %> 
        </div>
      <%})%>
    </div>
    <%- include("form.ejs") %>
  </body>
</html>
